            <data idref='alignment'/>
        </frequencies>
    </input-->

    <input spec='NtdBMA' id='ntdBMA'>
        <parameter id="logKappa" name="logKappa" value="0.0" lower="-Infinity" upper="Infinity"/>
        <parameter id="logTN" name="logTN" value="0.0" lower="-Infinity" upper="Infinity"/>
        <parameter id="logAC" name="logAC" value="0.0" lower="-Infinity" upper="Infinity"/>
        <parameter id="logAT" name="logAT" value="0.0" lower="-Infinity" upper="Infinity"/>
        <parameter id="logGC" name="logGC" value="0.0" lower="-Infinity" upper="Infinity"/>
        <parameter id="logGT" name="logGT" value="0.0" lower="-Infinity" upper="Infinity"/>
        <input spec='beast.core.parameter.IntegerParameter' id="modelChoose" name="modelChoose" value='4' lower='3' upper='5' dimension="1"/>
        <!--input spec='beast.core.parameter.IntegerParameter' id="modelChoose" name="modelChoose">
            <input name="value">0 0</input>
            <input name="lower">0</input>
            <input name="upper">1</input>
            <input name="dimension">2</input>
        </input-->
        <frequencies id='freqs' spec='Frequencies'>
            <data idref='alignment'/>
        </frequencies>
    </input>

    <!-- site model                                                              -->
    <input spec='SiteModel' id="siteModel">
        <substModel idref='ntdBMA'/>
    </input>

    <input spec='TreeLikelihood' id="treeLikelihood">
        <data idref="alignment"/>
        <tree idref="tree"/>
        <siteModel idref="siteModel"/>
    </input>


    <tree spec='beast.util.ClusterTree' id='tree' clusterType='upgma'>
        <taxa idref='alignment'/>
    </tree>


    <run spec="MCMC" id="mcmc" chainLength="1000000">
        <state storeEvery='100000'>
            <stateNode idref="logKappa"/>
            <stateNode idref="tree"/>
        </state>

        <distribution id='likelihood' idref="treeLikelihood"/>

        <operator id='logKappaRW' spec='RealRandomWalkOperator' windowSize="0.5" weight="1" parameter="@logKappa"/>
        <operator id='treeScaler' spec='ScaleOperator' scaleFactor="0.5" weight="1" tree="@tree"/>
        <operator spec='Uniform' weight="10" tree="@tree"/>
        <operator spec='SubtreeSlide' weight="5" gaussian="true" size="1.0" tree="@tree"/>
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="1" tree="@tree"/>
        <operator id='wide' spec='Exchange' isNarrow='false' weight="1" tree="@tree"/>
        <operator spec='WilsonBalding' weight="1" tree="@tree"/>

        <logger logEvery="1000">
	        <model idref='likelihood'/>
            <log idref="likelihood"/>
    	    <ESS spec='ESS' name='log' arg="@likelihood"/>
            <log idref='logKappa'/>
    	    <ESS spec='ESS' name='log' arg="@logKappa"/>
        </logger>
        <logger logEvery="1000" fileName="realDataBMA1.log">
	        <model idref='likelihood'/>
            <log idref="likelihood"/>
            <log idref='logKappa'/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
        </logger>
        <logger logEvery="10000" fileName="realDataBMA1.trees">
            <log idref="tree"/>
        </logger>
    </run>

</beast>
